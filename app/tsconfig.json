{
    "compilerOptions": {
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "isolatedModules": false,
        "jsx": "preserve",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "preserveConstEnums": true,
        "allowJs": false,
        "removeComments": false
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "../typings/**/*.d.ts"
    ],
    "files": [

    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "compileOnSave": false
}
